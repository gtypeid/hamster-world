# Lambda Proxy (GitHub API 프록시)
VITE_LAMBDA_URL=https://your-lambda-function-url.lambda-url.region.on.aws
VITE_GITHUB_OWNER=your_github_username
VITE_GITHUB_REPO=your_repo_name

# Infrastructure session config (optional - defaults shown)
VITE_RUNTIME_MIN=20
VITE_COOLDOWN_MIN=5
VITE_MAX_SESSIONS_PER_DAY=5
